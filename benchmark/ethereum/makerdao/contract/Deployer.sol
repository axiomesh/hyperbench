pragma solidity ^0.6.12;

abstract contract BaseDeployer {
    string public salt = "default";

    event Deployed(string name, address addr);

    constructor() public {
        deploy();
    }

    function setSalt(string memory _salt) public {
        salt = _salt;
    }

    function deploy() public virtual;

    function deployContract(string memory name, bytes memory bytecode)
    internal
    returns (address addr)
    {
        bytes32 s = keccak256(abi.encodePacked(salt));
        assembly {
            addr := create2(0, add(bytecode, 0x20), mload(bytecode), s)
            if iszero(extcodesize(addr)) {
                revert(0, 0)
            }
        }
        emit Deployed(name, addr);
        return addr;
    }

    function getChainID() internal pure returns (uint256 id) {
        assembly {
            id := chainid()
        }
    }
}

contract Deployer is BaseDeployer {
    // default: 0x847C5302e34997c11fc3C2d4b1dbDcaA83577E8f
    address public daiAddr;
    // default: 0x14d493c0b0213a8Cf33186b6C535152cECb3a17b
    address public erc20Addr;
    // default: 0xac9766576143a129fe779d6d43971E3236F9B747
    address public vatAddr;
    // default: 0x1B5D2dB0A94968a07d69590165D58BeFB511a4f3
    address public daiJoinAddr;
    // default: 0x6F6BB85b8aaAccC27d11492bEb21817DA49D5642
    address public gemJoinAddr;

    function deploy() public override {
        deployDai();
        deployErc20();
        deployVat();
        deployDaiJoin();
        deployGemJoin();
    }

    function deployDai() internal {
        bytes
        memory bytecode = hex"";
        daiAddr = deployContract(
            "dai",
            // encode with constructor parameters: constructor(uint256 chainId_, address sender)
            abi.encodePacked(bytecode, abi.encode(getChainID(), 0xC98FF0B7987937882AB2137b4075c51c442cB1EE))
        );
    }

    function deployErc20() internal {
        bytes
        memory bytecode = hex"";
        erc20Addr = deployContract(
            "erc20",
            // encode with constructor parameters: constructor(address sender, uint256 amount)
            abi.encodePacked(
                bytecode,
                abi.encode(0xC98FF0B7987937882AB2137b4075c51c442cB1EE, 1000000000000000000000)
            )
        );
    }

    function deployVat() internal {
        bytes
        memory bytecode = hex"";
        vatAddr = deployContract(
            "vat",
            // encode with constructor parameters: constructor(address sender)
            abi.encodePacked(bytecode, abi.encode(0xC98FF0B7987937882AB2137b4075c51c442cB1EE))
        );
    }

    function deployDaiJoin() internal {
        bytes
        memory bytecode = hex"";
        daiJoinAddr = deployContract(
            "daiJoin",
            // encode with constructor parameters: constructor(
            //        address sender,
            //        address vat_,
            //        address dai_
            //    )
            abi.encodePacked(bytecode, abi.encode(0xC98FF0B7987937882AB2137b4075c51c442cB1EE, vatAddr, daiAddr))
        );
    }

    function deployGemJoin() internal {
        bytes
        memory bytecode = hex"";
        gemJoinAddr = deployContract(
            "gemJoin",
            // encode with constructor parameters: constructor(
            //        address sender,
            //        address vat_,
            //        bytes32 ilk_,
            //        address gem_
            //    )
            abi.encodePacked(
                bytecode,
                abi.encode(
                    0xC98FF0B7987937882AB2137b4075c51c442cB1EE,
                    vatAddr,
                    keccak256(abi.encodePacked("")),
                    erc20Addr
                )
            )
        );
    }
}
